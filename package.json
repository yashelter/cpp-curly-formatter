{
  	"name": "cppcurlyformatter",
  	"displayName": "C++ Curly Formatter",
  	"description": "C++ formatting fixes collection.",
  	"version": "0.1.0",
  	"publisher": "yashelter",
  	"icon": "icon.png",
  	"engines": {
  	  	"vscode": "^1.75.0"
  	},
  	"categories": [
  	  	"Snippets",
  	  	"Formatters",
  	  	"Keymaps"
  	],
  	"main": "./out/extension.js",
  	"contributes": {
  	  	"commands": [
  	  	  	{
  	  	  	  	"command": "cppcurlyformatter.curlyformat",
  	  	  	  	"title": "Curly Format"
  	  	  	}
  	  	],
  	  	"keybindings": [
  	  	  	{
  	  	  	  	"key": "enter",
  	  	  	  	"command": "cppcurlyformatter.curlyformat",
  	  	  	  	"when": "(editorLangId == cpp || editorLangId == c || editorLangId == objective-c || editorLangId == objective-cpp) && editorTextFocus"
  	  	  	}
  	  	],
  	  	"configuration": {
  	  	  	"title": "C++ Curly Formatter",
  	  	  	"properties": {
  	  	  	  	"cppcurlyformatter.autoTabIndentation": {
  	  	  	  	  	"type": "boolean",
  	  	  	  	  	"description": "Enable automatic tab intendation style",
  	  	  	  	  	"default": true
  	  	  	  	}
  	  	  	}
  	  	}
  	},
  	"scripts": {
  	  	"vscode:prepublish": "npm run compile",
  	  	"compile": "tsc -p ./",
  	  	"watch": "tsc -watch -p ./",
  	  	"pretest": "npm run compile && npm run lint",
  	  	"lint": "eslint src --ext ts",
  	  	"test": "node ./out/test/runTest.js"
  	},
  	"devDependencies": {
  	  	"@types/glob": "^7.1.4",
  	  	"@types/mocha": "^9.0.0",
  	  	"@types/node": "14.x",
  	  	"@types/vscode": "^1.62.0",
  	  	"@typescript-eslint/eslint-plugin": "^5.1.0",
  	  	"@typescript-eslint/parser": "^5.1.0",
  	  	"@vscode/test-electron": "^1.6.2",
  	  	"esbuild": "^0.23.0",
  	  	"eslint": "^8.1.0",
  	  	"glob": "^7.1.7",
  	  	"mocha": "^9.2.2",
  	  	"typescript": "^4.4.4"
  	},
  	"license": "SEE LICENSE IN LICENSE.md",
  	"bugs": {
  	  	"url": "https://github.com/yashelter/cpp-curly-formatter/issues",
  	  	"email": "yashelteer@gmail.com"
  	},
  	"repository": {
  	  	"type": "git",
  	  	"url": "https://github.com/yashelter/cpp-curly-formatter/"
  	}
}
